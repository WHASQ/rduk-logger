  "bugs": {
    "url": "https://github.com/rd-uk/rduk-logger/issues"
  },
  "homepage": "https://github.com/rd-uk/rduk-logger#readme",
  "devDependencies": {
    "coveralls": "3.0.0",
    "istanbul": "0.4.5",
    "jasmine": "2.8.0",
    "jshint": "2.9.5"
  },
  "dependencies": {
    "@rduk/configuration": "2.0.1",
    "@rduk/provider": "2.0.0"
